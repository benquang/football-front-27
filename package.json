{
  "name": "hcmute-portal-fe",
  "version": "15.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "clean:git": "git fetch -p && for branch in $(git for-each-ref --format '%(refname) %(upstream:track)' refs/heads | awk '$2 == \"[gone]\" {sub(\"refs/heads/\", \"\", $1); print $1}'); do git branch -D $branch; done",
    "prepare": "husky install",
    "compodoc:build": "compodoc -p tsconfig.doc.json",
    "compodoc:build-and-serve": "compodoc -p tsconfig.doc.json -s",
    "compodoc:serve": "compodoc -s"
  },
  "husky": {
    "hooks": {
      "pre-commit": "echo \"[Husky] Prepare before commit...\"",
      "commitmsg": "echo \"[Husky] Validating commit name...\""
    }
  },
  "private": true,
  "dependencies": {
    "@abacritt/angularx-social-login": "^1.3.2",
    "@angular/animations": "^15.2.9",
