    "@sentry/angular-ivy": "^7.73.0",
    "csv-parser": "^3.0.0",
    "date-fns": "^2.30.0",
    "fs": "^0.0.1-security",
    "gapi-client": "^0.0.3",
    "google-auth-library": "^9.10.0",
    "ng-zorro-antd": "^15.1.0",
